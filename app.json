{
  "expo": {
    "name": "Helping-Hands-App-Novo",
    "slug": "Helping-Hands-App-Novo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/league-spartan/600SemiBold/LeagueSpartan_600SemiBold.ttf", "node_modules/@expo-google-fonts/league-spartan/100Thin/LeagueSpartan_100Thin.ttf", "node_modules/@expo-google-fonts/league-spartan/300Light/LeagueSpartan_300Light.ttf", "node_modules/@expo-google-fonts/league-spartan/400Regular/LeagueSpartan_400Regular.ttf", "node_modules/@expo-google-fonts/league-spartan/500Medium/LeagueSpartan_500Medium.ttf"]
        }
      ]
    ]
  }
}
